//!Create Static Member Function
//@A.cpp
struct X
{
};
void test() {
	X::foo();
}
//=
struct X
{
	static void foo() {
	}
};
void test() {
	X::foo();
}
//!Create Static Member Function with Return Type
//@A.cpp
struct X
{
};

void test() {
	X x = X::foo();
}
//=
struct X
{
	static X foo() const {
		return X();
	}
};

void test() {
	X x = X::foo();
}
//!Create Static Member Function with Return Type and Parameter
//@A.cpp
struct X
{
};

void test() {
	X x = X::foo(5);
}
//=
struct X
{
	static X foo(const int& i) const {
		return X();
	}
};

void test() {
	X x = X::foo(5);
}
//!Create Static Member Function in Header
//@A.cpp
#include "A.h"

void test() {
	X::foo();
}
//=
#include "A.h"

void test() {
	X::foo();
}
//@A.h
struct X
{
};
//=
struct X
{
	static void foo() {
	}
};
//!Create Static Member Function with Decltype
//@A.cpp
struct X
{
};
void test() {
	X x{};
	decltype(x)::foo();
}
//=
struct X
{
	static void foo() {
	}
};
void test() {
	X x{};
	decltype(x)::foo();
}