//!TestSimpleVariableCreation
//@A.cpp
void func()
{
    int x = 1;
    y = x;
}
//=
void func()
{
    int x = 1;
    int y;
    y = x;
}
//!TestNamedTypeVariableCreation
//@A.cpp
struct B
{
};

void func()
{
    B b1;
    b2 = b1;
}
//=
struct B
{
};

void func()
{
    B b1;
    B b2;
    b2 = b1;
}
//!SimpleMacro
//@A.cpp
#define MACRO(c) c

void test()
{
    MACRO(k);
}
//=
#define MACRO(c) c

void test()
{
    int k;
    MACRO(k);
}
//!NamedTypeVariable
