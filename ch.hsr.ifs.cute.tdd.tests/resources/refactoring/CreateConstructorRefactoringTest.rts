//!MultipleArgumentsJustOneParam
//@A.cpp
struct A {
     A(const int & i)
    {
    }
};

void test()
{
    A a(2, 2);
}
//=
struct A {
     A(const int & i, const int & j)
    {
    }

    A(const int & i)
    {
    }
};

void test()
{
    A a(2, 2);
}
