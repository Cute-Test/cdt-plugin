//!Create Constructor from Function Call Expression
//@A.cpp
struct Rnd {
    int getInt(){
        return 4; //Random number determined by rolling a die
    }
};

struct A {
};

void test()
{
    Rnd r;
    A a(r.getInt());
}
//=
struct Rnd {
    int getInt(){
        return 4; //Random number determined by rolling a die
    }
};

struct A {
     A(const int & rgetInt)
    {
    }
};

void test()
{
    Rnd r;
    A a(r.getInt());
}
