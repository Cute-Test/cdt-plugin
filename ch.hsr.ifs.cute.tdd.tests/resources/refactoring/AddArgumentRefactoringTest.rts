//!DontOfferAddArgsThatCantBeAppended
//@.config
nomarkers=true
//@A.cpp
#include<string>
void foo(std::string, int);
void test() {
  foo(42);
}
//=
#include<string>
void foo(std::string, int);
void test() {
  foo(42);
}
//!CheckStringandCharArrays
//@.config
nomarkers=true
//@A.cpp
#include <string>

void foo(std::string s)
{
}

void test() {
    foo("asdfs");
}
//=
#include <string>

void foo(std::string s)
{
}

void test() {
    foo("asdfs");
}
//!RemoveSingleLastArgument
//@A.cpp
void foo() {
}

void test() {
    foo(3);
}
//=
void foo() {
}

void test() {
    foo();
}
//!RemoveBunchLastArguments
//@A.cpp
void foo(int i) {
}

void test() {
    foo(3, 2.3, "asdf", 2);
}
//=
void foo(int i) {
}

void test() {
    foo(3);
}