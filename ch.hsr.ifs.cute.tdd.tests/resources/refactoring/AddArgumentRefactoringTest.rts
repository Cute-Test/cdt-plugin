//!AddIntegerArgument
//@A.cpp
void func(int i) { }
void testFunc() {
	func();
}
//=
void func(int i) { }
void testFunc() {
	func(_);
}
//!AddTwoIntegerArguments
//@A.cpp
void func(int i, int j) { }
void testFunc() {
	func();
}
//=
void func(int i, int j) { }
void testFunc() {
	func(_, _);
}
//!AddOneIntegerArgument
//@A.cpp
void func(int i, int j) { }
void testFunc() {
	func(0);
}
//=
void func(int i, int j) { }
void testFunc() {
	func(0, _);
}
//!InsertTypeArgument
//@.config
nomarkers=true
//@A.cpp
struct Type {};
void func(Type t, int i) { }
void testFunc() {
	func(0);
}
//=
struct Type {};
void func(Type t, int i) { }
void testFunc() {
	func(0);
}
//!RecognizeBasicTypesAsMatchingArguments
//@A.cpp
void func(char c, int i) { }
void testFunc() {
	func(42);
}
//=
void func(char c, int i) { }
void testFunc() {
	func(42, _);
}
//!RecognizeIds
//@A.cpp
void func(int i, int j) { }
void testFunc() {
	int i = 42;
	func(i);
}
//=
void func(int i, int j) { }
void testFunc() {
	int i = 42;
	func(i, _);
}
//!RecognizeFunctionCalls
//@A.cpp
void func(int i, int j) { }
int foo() {return 42;}
void testFunc() {
	func(foo());
}
//=
void func(int i, int j) { }
int foo() {return 42;}
void testFunc() {
	func(foo(), _);
}
