//!PreserveCommentsWhenHiding
//@A.cpp
struct B{
private:
    // comment above function
    int foo()
    {
        // comment inside function
    }
};
void test() {
    B b;
    int x = b.foo();
}
//=
struct B{
    // comment above function
    int foo()
    {
        // comment inside function
    }

private:
};
void test() {
    B b;
    int x = b.foo();
}
//!PreserveMacrosWhenHiding
//@A.cpp
class Klasse
{
    int foo(int i = __LINE__) {
        return __LINE__;
    }
};
int main() {
    Klasse k;
    int x = k.foo(42);
    return 0;
}
//=
class Klasse
{
public:
    int foo(int i = __LINE__) {
        return __LINE__;
    }
};
int main() {
    Klasse k;
    int x = k.foo(42);
    return 0;
}
//!TestChangeInOtherFiles
