//!CallPrivateInStructFromOutside
//@A.cpp
struct B
{
private:
    int foo()
    {
        return int();
    }
};

void test()
{
    B b;
    int x = b.foo();
}
//=
struct B
{
    int foo()
    {
        return int();
    }

private:
};

void test()
{
    B b;
    int x = b.foo();
}
