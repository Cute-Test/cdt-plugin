//!TestNestedTemplatedStructExtractionSelectionInTemplatedMember
//@.config
typeextraction=true
nomarkers=true
newfiles=B.h
ignorecomments=true
//@A.h
template<typename T, typename U>
struct B {
	T t;
	U u;
	template <typename W>
	W me/*$*//*$$*/mber() {
		return W();
	}
};
//=
#include "B.h"

//@B.h
//=
#ifndef B_H_
#define B_H_



template<typename T, typename U>
struct B {
	T t;
	U u;
	template <typename W>
	W member() {
		return W();
	}
};

#endif /* B_H_ */
