//!Array handling
//@.config
filename=A.cpp
//@SUT.h
namespace {
  struct A {
  };
}

template <typename S, typename T>
struct SUT {
  T fake;
  void bar() {
    int arr[] = { 16, 2, 77, 40, 12071 };
    fake.foo(arr);
    A aaas[3];
    fake.foo2(aaas);
  }
};

//@A.cpp
#include "SUT.h"

struct A {
};

void testSUT(){
  struct /*$*/Fake/*$$*/ {
  };
  SUT<A, Fake> *sut = new SUT<A, Fake>;
  sut->bar();
}
//=
#include "SUT.h"

struct A {
};

void testSUT(){
  struct Fake {
      void foo(const int arr[]) const
      {
      }
      void foo2(const A aaas[]) const
      {
      }
  };
  SUT<A, Fake> *sut = new SUT<A, Fake>;
  sut->bar();
}

