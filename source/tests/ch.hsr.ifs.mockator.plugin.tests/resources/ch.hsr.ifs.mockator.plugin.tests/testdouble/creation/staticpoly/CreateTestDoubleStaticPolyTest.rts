//!Into existing suite NS for C++03
//@.config
cppStandard=C++03
filename=A.cpp

//@A.cpp
#include "cute.h"
template<typename T>
struct SUT {};

namespace testSuite
{
	namespace testBar_Ns
	{
	    struct Bar
	    {
	    };
	}
}

void testBar() {
    using namespace testSuite::testBar_Ns;
	SUT<Bar> bar;
}

void testFoo() {
	SUT</*$*/Foo/*$$*/> foo;
}

void runSuite() {
	cute::suite testSuite;
	testSuite.push_back(CUTE(testBar));
	testSuite.push_back(CUTE(testFoo));
}

//=
#include "cute.h"
template<typename T>
struct SUT {};

namespace testSuite
{
	namespace testBar_Ns
	{
	    struct Bar
	    {
	    };
	}
    namespace testFoo_Ns
    {
        struct Foo
        {
        };}
}

void testBar() {
    using namespace testSuite::testBar_Ns;
	SUT<Bar> bar;
}

void testFoo() {
    using namespace testSuite::testFoo_Ns;
	SUT<Foo> foo;
}

void runSuite() {
	cute::suite testSuite;
	testSuite.push_back(CUTE(testBar));
	testSuite.push_back(CUTE(testFoo));
}

