//!New test double class for C++03
//@.config
cppStandard=C++03
filename=A.cpp

//@A.cpp
#include "cute.h"
template<typename T>
struct SUT {};

void testFoo() {
	SUT</*$*/Foo/*$$*/> foo;
}

void runSuite() {
	cute::suite testSuite;
	testSuite.push_back(CUTE(testFoo));
}
//=
#include "cute.h"
template<typename T>
struct SUT {};

namespace testSuite
{
    namespace testFoo_Ns
    {
        struct Foo
        {
        };
    }
}
void testFoo() {
    using namespace testSuite::testFoo_Ns;
	SUT<Foo> foo;
}

void runSuite() {
	cute::suite testSuite;
	testSuite.push_back(CUTE(testFoo));
}

