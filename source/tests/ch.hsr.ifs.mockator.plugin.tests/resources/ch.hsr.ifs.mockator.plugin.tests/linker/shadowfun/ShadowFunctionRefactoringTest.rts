class A {
  __attribute__((weak)) 
  void foo(int) const;
};
}

//@A.cpp
#include "A.h"

void bar() {
  Ns::A a;
  a./*$*/foo/*$$*/(42);
}

//@shadows/foo.cpp
//=
#include "../A.h"

void Ns::A::foo(int i) const
{
}

//!Shadow free function in macro
//@.config
newFiles=shadows/foo.cpp
newFileCreation=true
filename=A.cpp
needsManagedCProject=true

//@foo.h
bool foo(char c) {
}
//=
__attribute__((weak)) 
bool foo(char c) {
}
 
//@A.cpp
#include "foo.h"

void test() {
  ASSERT(/*$*/foo('c')/*$$*/);
}

//@shadows/foo.cpp
//=
#include "../foo.h"
bool foo(char c)
{
    return bool();
}