//!Shadow free function
//@.config
newFiles=shadows/foo.cpp
filename=A.cpp
newFileCreation=true
needsManagedCProject=true

//@foo.h
class A {
};
int foo(A const& a, int * ip, char c) {
}
//=
class A {
};
__attribute__((weak)) int foo(const A& a, int* ip, char c) {
}
 
//@A.cpp
#include "foo.h"
#include "gogo.h"

int bar() {
  A a;
  int i = 3;
  int *ip = &i;
  return /*$*/foo/*$$*/(a, ip, 'd');
}

//@shadows/foo.cpp
//=
#include "../foo.h"
int foo(const A & a, int *ip, char c)
{
    return int();
}

//!Shadow member function
//@.config
newFiles=shadows/foo.cpp
newFileCreation=true
filename=A.cpp
needsManagedCProject=true

//@Foo.h
class Bar;

class Foo {
  int foo(Bar const& boo) const;
};
//=
class Bar;

class Foo {
    __attribute__((weak)) int foo(const Bar& boo) const;
};

//@Foo.cpp
#include "Foo.h"
#include <string>
#include <map>

int Foo::foo(Bar const& boo) const
{
  return 42;
}

//@A.cpp
#include "Foo.h"

int bar() {
  Foo f;
  Bar b;
  return f./*$*/foo/*$$*/(b);
}

//@shadows/foo.cpp
//=
#include "../Foo.h"
int Foo::foo(const Bar & boo) const
{
    return int();
}

