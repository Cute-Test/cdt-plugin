//=
#include <cstring>
#include <string>
#include "iostream.h"

int main(){
	std::string mail = "admin@web.com";
	std::string::size_type mail_pos = 0;
	while (mail.find('@', mail_pos) != std::string::npos) {
		mail_pos++;
	}
	std::cout << (mail.c_str() + mail_pos) << std::endl;
}


