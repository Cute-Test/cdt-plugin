//!StrcspnAssignedToVariable
//@main.cpp
#include <cstring>
#include <iostream>

int main() {
	char str[] = "this is a string";
	size_t first_digit = strcspn(str, "0123456789");
	std::cout << first_digit << std::endl;
}
//=
#include <cstring>
#include <iostream>
#include <string>

int main() {
	std::string str = "this is a string";
	size_t first_digit = strcspn(str.c_str(), "0123456789");
	std::cout << first_digit << std::endl;
}


