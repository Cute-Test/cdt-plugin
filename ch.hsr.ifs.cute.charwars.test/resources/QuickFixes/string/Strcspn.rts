	char str[] = "this is a string";
	const char *digits = "0123456789";
	if(strcspn(str, digits) == 5) {}
	while(strcspn(str, digits) == 5) {}
	bool x = strcspn(str, digits) == 5;
	if(strcspn(str, digits) != 5) {}
	while(strcspn(str, digits) != 5) {}
	bool x = strcspn(str, digits) != 5;
}
