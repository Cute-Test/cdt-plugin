//!StrncmpFirstArg
//@main.cpp
#include <cstring>
int main() {
	char *a = "String1";
	char *b = "String2";
	int n = strncmp(a, b, 6);
}
//=
#include <cstring>
#include <string>
int main() {
	std::string a = "String1";
	char *b = "String2";
	int n = a.compare(0, 6, b, 0, 6);
}


//!StrncmpFirstArgWithString
//@main.cpp
#include <cstring>
#include <string>
int main() {
	char *a = "String1";
	std::string b = "String2";
	int n = strncmp(a, b.c_str(), 6);
}
//=
#include <cstring>
#include <string>
int main() {
	std::string a = "String1";
	std::string b = "String2";
	int n = a.compare(0, 6, b, 0, 6);
}


//!StrncmpFirstArgWithOffset
//@main.cpp
#include <cstring>
int main() {
	char *a = "TestString1";
	char *b = "String2";
	int n = strncmp(a + 4, b, 6);
}
//=
#include <cstring>
#include <string>
int main() {
	std::string a = "TestString1";
	char *b = "String2";
	int n = a.compare(4, 6, b, 0, 6);
}


