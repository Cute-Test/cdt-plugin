//!StrspnComparedToNumber
//@main.cpp
#include <cstring>
#include <iostream>

int main() {
	char str[] = "this is a string";
	const char *digits = "0123456789";
	if(strspn(str, digits) == 5) {}
	while(strspn(str, digits) == 5) {}
	bool x = strspn(str, digits) == 5;
	if(strspn(str, digits) != 5) {}
	while(strspn(str, digits) != 5) {}
	bool x = strspn(str, digits) != 5;
}
