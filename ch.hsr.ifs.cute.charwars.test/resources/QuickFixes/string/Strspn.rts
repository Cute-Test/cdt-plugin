//!StrspnDefault
//@main.cpp
#include <cstring>
#include <iostream>

int main() {
	char str[] = "this is a string";
	size_t first_non_letter = strspn(str, "abcdefghijklmnopqrstuvwxyz ");
}
//=
#include <cstring>
#include <iostream>
#include <string>

int main() {
	std::string str = "this is a string";
	size_t first_non_letter = strspn(str.c_str(),
			"abcdefghijklmnopqrstuvwxyz ");
}


