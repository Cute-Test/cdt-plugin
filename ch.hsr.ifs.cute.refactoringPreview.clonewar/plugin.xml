<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.actionSets">
      <actionSet
            id="ch.hsr.cdt.clonewar.app.actionSet"
            label="%actionSet.label">
         <menu
               id="org.eclipse.jdt.ui.refactoring.menu"
               label="%menu.label">
            <separator
                  name="codingGroup">
            </separator>
         </menu>
         <action
               class="ch.hsr.ifs.cute.refactoringPreview.clonewar.app.CloneWarActionDelegate"
               definitionId="ch.hsr.ifs.cute.refactoringPreview.clonewar.command"
               id="ch.hsr.cdt.clonewar"
               label="%action.label"
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/codingGroup">
            <enablement>
               <objectClass
                     name="org.eclipse.jface.text.ITextSelection">
               </objectClass>
            </enablement>
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.cdt.ui.category.refactoring"
            id="ch.hsr.ifs.cute.refactoringPreview.clonewar.command"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
         <viewerContribution
            id="ch.hsr.ifs.cute.tdd.cEditorContribution"
            targetID="#CEditorContext">
         <action
               class="ch.hsr.ifs.cute.refactoringPreview.clonewar.app.CloneWarActionDelegate"
               definitionId="ch.hsr.ifs.cute.refactoringPreview.clonewar.command"
               id="ch.hsr.ifs.cute.refactoring.clonewar"
               label="%action.label"
               menubarPath="org.eclipse.cdt.ui.refactoring.menu/codingGroup">
         </action>
      </viewerContribution>
   </extension>

</plugin>
