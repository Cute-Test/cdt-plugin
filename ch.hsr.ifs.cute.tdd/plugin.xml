<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="ch.hsr.ifs.cute.tdd.addArgument.AddArgumentQFGenerator"
            markerType="org.eclipse.cdt.codan.core.codanProblem">
      </markerResolutionGenerator>
      
      <markerResolutionGenerator
            class="ch.hsr.ifs.cute.tdd.addArgument.AddArgumentQFGenerator"
            markerType="org.eclipse.cdt.codan.core.codanSemanticProblem">
      </markerResolutionGenerator>
      
      <markerResolutionGenerator
            class="ch.hsr.ifs.cute.tdd.addArgument.AddArgumentQFGenerator"
            markerType="org.eclipse.cdt.core.problem">
      </markerResolutionGenerator>
   </extension>

   <extension point="org.eclipse.cdt.codan.ui.codanMarkerResolution">
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.FreeOperatorCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.MissingOperatorResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createtype.CreateTypeQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.TypeResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.NormalFunctionCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.MethodResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.NormalFreeFunctionCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.FunctionResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.CreateConstructorCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.MissingConstructorResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createvariable.CreateMemberVariableQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.MemberVariableResolutionProblem_HSR">
      </resolution>
      <resolution
             class="ch.hsr.ifs.cute.tdd.createvariable.CreateLocalVariableQuickFix"
             problemId="ch.hsr.ifs.cute.tdd.codan.checkers.VariableResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.changevisibility.ChangeVisibilityQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.PrivateMethodChecker_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.MemberOperatorCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.MissingOperatorResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.FreeOperatorCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.MissingOperatorResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.NormalFreeFunctionCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.InvalidArguments_FREE_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createnamespace.CreateNamespaceQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.NamespaceResolutionProblem_HSR">
      </resolution>
      <resolution
            class="ch.hsr.ifs.cute.tdd.createtype.CreateTypeQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.NamespaceResolutionProblem_HSR">
      </resolution>
       <resolution
            class="ch.hsr.ifs.cute.tdd.createfunction.quickfixes.StaticFunctionCreationQuickFix"
            problemId="ch.hsr.ifs.cute.tdd.codan.checkers.FunctionResolutionProblem_STATIC_HSR">
      </resolution>
   </extension>
   
  <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.FreeFunctionProblemChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.freefunctionproblemchecker"
           name="%checker.name">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.FunctionResolutionProblem_HSR"
           name="%problem.name">
        </problem>
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.FunctionResolutionProblem_STATIC_HSR"
           name="%problem.name.0">
        </problem>
     </checker>
  </extension>
  
   <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.MemberFunctionProblemChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.memberfunctionproblemchecker"
           name="%checker.name.0">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.MethodResolutionProblem_HSR"
           name="%problem.name.1">
        </problem>
     </checker>
  </extension>
  
  <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.WrongArgumentChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.wrongargumentchecker"
           name="%checker.name.1">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.eclipse.cdt.codan.checkers.InvalidArguments_HSR"
           name="%problem.name.2">
        </problem>
         <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.InvalidArguments_FREE_HSR"
           name="%problem.name.3">
        </problem>
     </checker>
  </extension>
  
   <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.TypeResolutionProblemChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.typeresolutionproblemchecker"
           name="%checker.name.2">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.TypeResolutionProblem_HSR"
           name="%problem.name.4">
        </problem>
     </checker>
  </extension>
  
  <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.VariableResolutionProblemChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.variableresolutionproblemchecker"
           name="%checker.name.3">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.VariableResolutionProblem_HSR"
           name="%problem.name.5">
        </problem>
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.MemberVariableResolutionProblem_HSR"
           name="%problem.name.6">
        </problem>
     </checker>
  </extension>

    <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.PrivateMethodChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.privatemethodchecker"
           name="%checker.name.4">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.PrivateMethodChecker_HSR"
           name="%problem.name.7">
        </problem>
     </checker>
  </extension>

    <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.MissingConstructorChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.missingconstructorchecker"
           name="%checker.name.5">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.MissingConstructorResolutionProblem_HSR"
           name="%problem.name.8">
        </problem>
     </checker>
  </extension>
  
  <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.MissingOperatorChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.missingoperatorchecker"
           name="%checker.name.6">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.MissingOperatorResolutionProblem_HSR"
           name="%problem.name.9">
        </problem>
     </checker>
  </extension>
  
  <extension
        point="org.eclipse.cdt.codan.core.checkers">
     <checker
           class="ch.hsr.ifs.cute.tdd.codan.checkers.MissingNamespaceChecker"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.missingnamespacechecker"
           name="%checker.name.7">
        <problem
           defaultSeverity="Error"
           id="ch.hsr.ifs.cute.tdd.codan.checkers.NamespaceResolutionProblem_HSR"
           name="%problem.name.10">
        </problem>
     </checker>
  </extension>
  
  <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.cdt.ui.category.refactoring"
            id="ch.hsr.ifs.cute.tdd.extractCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="ch.hsr.ifs.cute.tdd.extractCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="SHIFT+ALT+P">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="%actionSet.description"
            id="ch.hsr.ifs.cute.tdd.actionSet"
            label="%actionSet.label"
            visible="true">
         <menu
               id="org.eclipse.jdt.ui.refactoring.menu"
               label="%menu.label"
               path="edit">
            <separator
                  name="codeGroup">
            </separator>
         </menu>
         <action
               class="ch.hsr.ifs.cute.tdd.extract.ExtractActionDelegate"
               definitionId="ch.hsr.ifs.cute.tdd.extractCommand"
               id="ch.hsr.ifs.cute.tdd.action4"
               label="%action.label"
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/codeGroup"
               style="push">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <viewerContribution
            id="ch.hsr.ifs.cute.tdd.cEditorContribution"
            targetID="#CEditorContext">
         <action
               class="ch.hsr.ifs.cute.tdd.extract.ExtractActionDelegate"
               definitionId="ch.hsr.ifs.cute.tdd.extractCommand"
               id="ch.hsr.ifs.cute.tdd.action1"
               label="%action.label"
               menubarPath="org.eclipse.cdt.ui.refactoring.menu/reorgGroup">
         </action>
         <menu
               id="ch.hsr.ifs.cute.tdd.refactormenu"
               label="%menu.label.0"
               path="org.eclipse.cdt.ui.refactoring.menu">
            <separator
                  name="reorgGroup">
            </separator>
         </menu>
      </viewerContribution>
   </extension>
</plugin>
