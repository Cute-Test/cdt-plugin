//!testUnregisteredTestFunctionWithUsingDeclaration
//@.config
markerLineNr=4
//@main.cpp
 #define ASSERTM(msg,cond) if (!(cond)) throw cute::test_failure((msg),__FILE__,__LINE__)
 #define ASSERT(cond) ASSERTM(#cond,cond)

 void thisIsAUnregisteredTest(){
 ASSERT(true);
 }

 void runSuite() {
 using cute::suite;
 suite s;
 }
